{
  "schemaVersion": 1,
  "id": "ntm",
  "version": "${version}",
  "name": "NTM-Fabric",
  "description": "Recreation of HBM's Nuclear Tech Mod from 1.7.10 to Modern Fabric",
  "authors": [
    "fawnoculus"
  ],
  "contact": {
    "sources": "https://github.com/fawnoculus/NTM-Fabric",
    "issues": "https://github.com/fawnoculus/NTM-Fabric/issues"
  },
  "license": "GPL-3.0",
  "icon": "assets/ntm/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "net.fawnoculus.ntm.main.NTMDataGenerator"
    ],
    "client": [
      "net.fawnoculus.ntm.main.NTMClient"
    ],
    "main": [
      "net.fawnoculus.ntm.main.NTM"
    ]
  },
  "mixins": [
    "ntm.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
